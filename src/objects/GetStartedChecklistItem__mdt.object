        <valueSet>
            <restricted>true</restricted>
            <valueSetDefinition>
                <sorted>false</sorted>
                <value>
                    <fullName>Disabled</fullName>
                    <default>true</default>
                    <label>Disabled</label>
                </value>
                <value>
                    <fullName>sfdc:new-tab</fullName>
                    <default>false</default>
                    <label>SFDC New Tab</label>
                </value>
                <value>
                    <fullName>link</fullName>
